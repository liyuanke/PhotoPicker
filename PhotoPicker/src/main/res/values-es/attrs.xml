<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义属性 -->
    <declare-styleable name="__picker_ClipViewLayout">
        <attr name="__picker_clipType" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="__picker_mHorizontalPadding" format="dimension" />
        <attr name="__picker_clipBorderWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="__picker_CircleImageView">
        <attr name="__picker_civ_border_width" format="dimension" />
        <attr name="__picker_civ_border_color" format="color" />
        <attr name="__picker_civ_border_overlay" format="boolean" />
        <attr name="__picker_civ_fill_color" format="color" />
    </declare-styleable>

</resources>